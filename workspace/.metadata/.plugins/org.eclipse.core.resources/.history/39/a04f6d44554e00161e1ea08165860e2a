package com.model;

public class Userbusiness implements java.io.Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private int id_business;//用户id
	private String name;//用户名
	private String password;//用户密码
	
	public Userbusiness() {//默认构造方法
	}
	
	public Userbusiness(int id_business, String name, String password) {//初始化属性
		this.id_business = id_business;
		this.name = name;
		this.password = password;
	}
	
	public int getId_business() {
		return id_business;
	}
	
	public void setId_business(int id_business) {
		this.id_business = id_business;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getPassword() {
		return password;
	}
	
	public void setPassword(String password) {
		this.password = password;
	}
}
